{
  "name": "babeljs-loose-spec",
  "version": "1.0.0",
  "description": "Show some edge cases of Babel",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npm run clean && babel src --out-dir lib",
    "test": "ava",
    "test:src": "ava src",
    "test:lib": "ava lib",
    "lint": "standard",
    "lint:fix": "standard --fix"
  },
  "author": "xiaody",
  "license": "MIT",
  "ava": {
    "verbose": true,
    "babel": {
      "testOptions": {
        "babelrc": false
      }
    }
  },
  "dependencies": {
    "@babel/cli": "^7",
    "@babel/preset-env": "^7.2.3",
    "ava": "^1.0.1",
    "standard": "^12.0.1"
  }
}
